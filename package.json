{
  "name": "photo_sharing_app",
  "version": "1.0.0",
  "description": "instagram clone",
  "author": "Bob",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "scripts": {
    "build": "nest build",
    "dev": "npm run start:dev",
    "lint": "eslint . --ext ts --ext js --fix",
    "postinstall": "husky init",
    "prebuild": "rimraf dist",
    "prod": "npm run start:prod",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:prod": "npm run build && NODE_ENV=production npm start",
    "swagger": "npm ts-node -r tsconfig-paths/register ./src/utils/swagger.ts --files ./env.d.ts",
    "test": "npm test:unit --passWithNoTests && pnpm test:e2e --passWithNoTests",
    "test:e2e": "jest --config ./test/jest.e2e.config.js",
    "test:unit": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "seed:run": "node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./dist/modules/app/seeder.js",
    "migration:create": "npm run prebuild && npm run build && npx typeorm-ts-node-esm migration:create",
    "migration:generate": "npm run prebuild && npm run build && npx typeorm-ts-node-esm migration:generate -d ./dist/modules/app/migration-data-source.js",
    "migration:run": "npm run prebuild && npm run build && npx typeorm-ts-node-esm migration:run -d ./dist/modules/app/migration-data-source.js",
    "migration:revert": "npm run prebuild && npm run build && npx typeorm-ts-node-esm migration:revert -d ./dist/modules/app/migration-data-source.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.733.0",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/common": "^10.4.9",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.9",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.9",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "@theinternetfolks/snowflake": "^1.3.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.8",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.29.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cluster": "^0.7.7",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "google-auth-library": "^9.15.0",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "nanoid": "3.3.7",
    "needle": "^3.3.1",
    "nestjs-cls": "^4.4.1",
    "nestjs-mailgun": "^4.0.3",
    "nestjs-paginate": "^8.6.3",
    "nestjs-pino": "^4.1.0",
    "papaparse": "^5.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pino": "^8.21.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.3.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "terser-webpack-plugin": "^5.3.10",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.3"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.9",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.3.14",
    "@swc/core": "1.4.13",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.16",
    "@types/papaparse": "^5.3.15",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "supertest": "^6.3.4",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.2",
    "webpack": "^5.96.1"
  }
}
